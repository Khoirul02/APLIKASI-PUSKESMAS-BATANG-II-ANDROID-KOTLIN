<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.aplikasi.pusk_batang">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/dinkes"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true">
        <activity android:name=".activity.PopUpAmbilAntrianActivity"></activity>
        <activity android:name=".activity.AmbilAntrianActivity" />
        <activity android:name=".activity.AntrianActivity" />
        <activity android:name=".activity.PoliActivity" />
        <activity android:name=".activity.DokterActivity" />
        <activity android:name=".dashboard.DashboardActivity" />
        <activity android:name=".RegistrasiActivity" />
        <activity android:name=".MainActivity" />
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>